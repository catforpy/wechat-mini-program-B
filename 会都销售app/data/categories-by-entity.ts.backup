// ==================== 导出所有主体类型的数据 ====================
export const categoriesByEntity: CategoriesByEntity[] = [
  {
    entityType: EntityType.COMPANY,
    entityName: '企业/政府等非个人主体',
    categories: companyCategories
  },
  {
    entityType: EntityType.PERSONAL,
    entityName: '个人主体',
    categories: personalCategories
  },
  {
    entityType: EntityType.OVERSEAS,
    entityName: '境外主体',
    categories: overseasCategories
  }
]

// 根据主体类型获取类目的辅助函数
export function getCategoriesByEntityType(entityType: EntityType): FirstLevelCategory[] {
  const data = categoriesByEntity.find(item => item.entityType === entityType)
  return data?.categories || []
}

